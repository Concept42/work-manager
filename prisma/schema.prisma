// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./file.db"
}

model Customer {
  id String @id @default(cuid())
  firstName String
  lastName String
  companyName String
  email String @unique
  adress String
  city String
  oib Int
  phoneNumber Int
}